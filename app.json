{
  "expo": {
    "name": "QueueHub",
    "slug": "queuehub",
    "description": "A mobile app built with React Native that allows users to join a queue's 'room' and add songs to the queue, users can also favourite songs to boost them higher in the queue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins":[
      [
        "expo-image-picker",
        {
          "photosPermission": "The app can access your photos"
        }
      ]

    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
